page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.userinfo {
  width: 100%;
  height: 300rpx;
  margin-top: 100rpx;
}
.userinfo .userinfo-avatar {
  width: 256rpx;
  height: 256rpx;
  border-radius: 50%;
}
.userinfo .userinfo-nickname {
  text-align: center;
  display: block;
  margin-top: 20rpx;
}
.login {
  display: block;
  margin-top: 200rpx;
  text-align: center;
}
